{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "expo start",
    "test": "jest",
    "lint": "./node_modules/.bin/eslint index.js ./src/* ./__tests__/*",
    "flow": "flow",
    "generate-default-config": "[ -f config.json ] || cp config.json.example config.json",
    "validateci": "./scripts/validate-ci-config",
    "ci:deploy": "./ci/deploy-to-expo.sh",
    "ci:post-expo-comment": "./ci/post-expo-comment.js",
    "storybook": "concurrently -k --raw 'storybook start -p 7007 --skip-packager' 'REACT_NATIVE_USE_STORYBOOK=1 yarn start'",
    "prestart": "yarn generate-default-config",
    "prelint": "yarn generate-default-config",
    "pretest": "yarn generate-default-config",
    "preci:deploy": "yarn generate-default-config",
    "prestorybook": "rnstl",
    "postinstall": "rndebugger-open --expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^8.0.0",
    "@theliturgists/redux-orm-proptypes": "^0.1.1",
    "axios": "^0.18.0",
    "expo": "^31.0.4",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "moment-duration-format": "^2.2.2",
    "moment-timezone": "^0.5.14",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.0",
    "react": "16.5.0",
    "react-moment-proptypes": "^1.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.1.tar.gz",
    "react-native-dropdownalert": "^3.7.0",
    "react-native-elements": "^0.19.0",
    "react-native-markdown-renderer": "^3.2.8",
    "react-native-screens": "^1.0.0-alpha.16",
    "react-native-slider": "^0.11.0",
    "react-navigation": "^3.0.5",
    "react-navigation-tabs": "^1.0.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-observable": "^0.17.0",
    "redux-orm": "^0.11.0",
    "rxjs": "^5.5.6"
  },
  "devDependencies": {
    "@octokit/rest": "^15.2.3",
    "@storybook/addon-actions": "^3.3.10",
    "@storybook/addon-links": "^3.3.10",
    "@storybook/addons": "^3.3.10",
    "@storybook/react-native": "^3.3.10",
    "axios-mock-adapter": "^1.10.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-expo": "^5",
    "concurrently": "^3.5.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0",
    "factory-girl": "^5.0.2",
    "flow-bin": "0.53.0",
    "jest": "23.2.0",
    "jest-expo": "^31.0.0",
    "jest-junit": "^3.4.1",
    "json": "^9.0.6",
    "jsonapi-server": "^4.2.0",
    "pluralize": "^7.0.0",
    "pre-commit": "^1.2.2",
    "react-dom": "16.0.0-beta.5",
    "react-native-debugger-open": "^0.3.15",
    "react-native-storybook-loader": "^1.7.0",
    "react-test-renderer": "16.0.0-beta.5",
    "source-map-support": "^0.5.6"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation|@?expo)"
    ]
  },
  "comment": "flow is disabled in pre-commit because react-navigation breaks it",
  "pre-commit": [
    "lint",
    "test",
    "validateci"
  ],
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./storybook/stories"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
