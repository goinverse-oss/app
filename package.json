{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "./node_modules/.bin/eslint index.js ./src/* ./__tests__/*",
    "flow": "flow",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "generate-default-config": "[ -f config.json ] || cp config.json.example config.json",
    "generate-icons": "app-icon generate -i ./assets/icon.png",
    "validateci": "./scripts/validate-ci-config",
    "ci:deploy": "./ci/deploy-to-expo.sh",
    "ci:post-expo-comment": "./ci/post-expo-comment.js",
    "storybook": "concurrently -k --raw 'storybook start -p 7007 --skip-packager' 'REACT_NATIVE_USE_STORYBOOK=1 yarn start'",
    "prestart": "yarn generate-default-config",
    "prelint": "yarn generate-default-config",
    "pretest": "yarn generate-default-config",
    "preci:deploy": "yarn generate-default-config",
    "prestorybook": "rnstl",
    "postinstall": "jetify && rndebugger-open --expo; patch-package; yarn pod",
    "pod": "(cd ios; hash pod 2>/dev/null && pod install || echo 'Cocoapods not installed; skipping `pod install`')",
    "splash": "./scripts/splash.js"
  },
  "dependencies": {
    "@brlja/react-native-slider": "^0.12.1",
    "@expo/vector-icons": "^10.0.0",
    "@react-native-community/art": "^1.1.2",
    "@react-native-community/async-storage": "^1.7.1",
    "@theliturgists/patreon-pledge": "^0.2.0",
    "@theliturgists/redux-orm-proptypes": "^0.1.1",
    "app-icon": "^0.13.0",
    "axios": "^0.18.0",
    "axios-retry": "^3.1.2",
    "contentful": "^7.1.0",
    "cycle": "^1.0.3",
    "expo": "^37.0.0",
    "expo-av": "~8.1.0",
    "expo-constants": "^9.0.0",
    "expo-file-system": "~8.1.0",
    "expo-keep-awake": "~8.1.0",
    "expo-linear-gradient": "~8.1.0",
    "expo-secure-store": "~8.1.0",
    "expo-splash-screen": "^0.2.0",
    "expo-updates": "~0.1.0",
    "expo-web-browser": "~8.1.0",
    "html-entities": "^1.2.1",
    "is-html": "^1.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "moment-duration-format": "^2.2.2",
    "moment-timezone": "^0.5.14",
    "numeral": "^2.0.6",
    "path-browserify": "^1.0.0",
    "prop-types": "^15.6.0",
    "qs": "^6.6.0",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-moment-proptypes": "^1.5.0",
    "react-native": "~0.61.4",
    "react-native-app-intro-slider": "^2.0.1",
    "react-native-dropdownalert": "^3.7.0",
    "react-native-elements": "^0.19.0",
    "react-native-firebase": "~5",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-markdown-renderer": "^3.2.8",
    "react-native-music-control": "^0.10.4",
    "react-native-progress": "^4",
    "react-native-reanimated": "~1.7.0",
    "react-native-render-html": "^4.2",
    "react-native-screens": "~2.2.0",
    "react-native-text-ticker": "^0.18.0",
    "react-native-unimodules": "~0.9.0",
    "react-native-web": "~0.11.7",
    "react-native-webview": "8.1.1",
    "react-navigation": "^3.11.0",
    "react-navigation-tabs": "^1.0.1",
    "react-redux": "^7",
    "redux": "^4",
    "redux-actions": "^2.2.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-observable": "^1",
    "redux-orm": "^0.11.0",
    "redux-persist": "^5.10.0",
    "remarkable": "^1.7.1",
    "rxjs": "^6",
    "rxjs-compat": "^6",
    "sentry-expo": "^1.13.0",
    "url-parse": "^1.4.4"
  },
  "devDependencies": {
    "@babel/core": "~7.8.6",
    "@octokit/rest": "^15.2.3",
    "@storybook/addon-actions": "^3.3.10",
    "@storybook/addon-links": "^3.3.10",
    "@storybook/addons": "^3.3.10",
    "@storybook/react-native": "^3.3.10",
    "axios-mock-adapter": "^1.10.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "~24.9.0",
    "babel-plugin-functional-hmr": "^1.0.24",
    "babel-preset-expo": "^8.1.0",
    "concurrently": "^3.5.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0",
    "factory-girl": "^5.0.2",
    "flow-bin": "0.53.0",
    "jest": "~24.9.0",
    "jest-expo": "^37.0.0",
    "jest-junit": "^3.4.1",
    "json": "^9.0.6",
    "metro-react-native-babel-preset": "~0.56.4",
    "patch-package": "^6.1.2",
    "pluralize": "^7.0.0",
    "postinstall-postinstall": "^2.0.0",
    "pre-commit": "^1.2.2",
    "react-dom": "16.9.0",
    "react-native-debugger-open": "^0.3.19",
    "react-native-storybook-loader": "^1.7.0",
    "react-test-renderer": "16.0.0-beta.5",
    "reactotron-react-native": "^3",
    "reactotron-redux": "^3",
    "rxjs-marbles": "^5.0.2",
    "source-map-support": "^0.5.6"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "./testSetup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|unimodules-permissions-interface|react-clone-referenced-element|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|sentry-expo|native-base|@unimodules|@react-native-community/async-storage))"
    ]
  },
  "comment": "flow is disabled in pre-commit because react-navigation breaks it",
  "pre-commit": [
    "lint",
    "test",
    "validateci"
  ],
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./storybook/stories"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
